        <table>
            <tr>
                <td>ID</td>
                <td>:<%= id %></td>
            </tr>
            <tr>
                <td>Name</td>
                <td>:<%= empName %></td>
            </tr>
            <tr>
                <td>Phone</td>
                <td>:<%= empPhone %></td>
